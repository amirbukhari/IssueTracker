{
  "name": "issuetracker",
  "displayName": "issuetracker",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:issuetracker.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "issue-tracker",
          "title": "Issue Tracker",
          "icon": "src/media/logo.png"
        }
      ]
    },
    "views": {
      "issue-tracker": [
        {
          "id": "issues",
          "name": "Issues",
          "icon": "src/media/logo.png",
          "contextualTitle": "Issue Explorer"
        }
      ]
    },
    "commands": [
      {
        "command": "issuetracker.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/node-fetch": "^2.5.11",
    "@types/vscode": "^1.58.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "@jamesgmarks/utilities": "^0.4.17",
    "node-env": "^0.1.6",
    "node-fetch": "^2.6.1"
  }
}
