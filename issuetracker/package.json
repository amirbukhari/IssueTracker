{
  "name": "issuetracker",
  "displayName": "issuetracker",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:issues",
    "onCommand:issuetracker.helloWorld",
    "onCommand:issuetracker.clickLink",
    "onCommand:issuetracker.createIssue",
    "onCommand:issuetracker.getIssues"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "issuetracker.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "issuetracker.clickLink",
        "title": "Click Link"
      },
      {
        "command": "issuetracker.createIssue",
        "title": "Create Issue"
      },
      {
        "command": "issuetracker.getIssues",
        "title": "Get Issues"
      },
      {
        "command": "issuetracker.refreshEntry",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      }
    ],
    "views": {
      "issues": [
        {
          "id": "issues",
          "name": "Issues",
          "icon": "src/media/logo.png",
          "contextualTitle": "Issues Explorer"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "issues",
          "title": "Issues",
          "icon": "src/media/logo.png"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "issues.clickLink",
          "when": "view == issues && viewItem == issue",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "@jamesgmarks/utilities": "^0.4.15",
    "@types/node-fetch": "^2.5.10",
    "node-fetch": "^2.6.1"
  }
}